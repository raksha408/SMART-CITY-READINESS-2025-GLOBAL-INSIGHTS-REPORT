    <DataSet Name="ds_GlobalKPIs">
      <Query>
        <DataSourceName>SmartCityDB_Source</DataSourceName>
        <CommandText>SELECT
    AVG(Sustainability_Readiness_Score) AS Avg_Readiness,
    AVG(Renewable_Energy_Pct) AS Avg_Renewable,
    AVG(Waste_Recycling_Pct) AS Avg_Recycling,
    AVG(AI_Integration_Level) AS Avg_AI_Level,
    MAX(Sustainability_Readiness_Score) AS Max_Readiness,
    MIN(Sustainability_Readiness_Score) AS Min_Readiness
FROM dbo.v_CityReadiness;
</CommandText>
      </Query>
      <Fields>
        <Field Name="Avg_Readiness">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Avg_Readiness</DataField>
        </Field>
        <Field Name="Avg_Renewable">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Avg_Renewable</DataField>
        </Field>
        <Field Name="Avg_Recycling">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Avg_Recycling</DataField>
        </Field>
        <Field Name="Avg_AI_Level">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Avg_AI_Level</DataField>
        </Field>
        <Field Name="Max_Readiness">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Max_Readiness</DataField>
        </Field>
        <Field Name="Min_Readiness">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Min_Readiness</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ds_Correlation_AI_Citizen">
      <Query>
        <DataSourceName>SmartCityDB_Source</DataSourceName>
        <CommandText>SELECT
    (SUM( (AI_Integration_Level - AVG_AI) * (Citizen_Satisfaction_Pct - AVG_CS) ) /
     ((COUNT(*) - 1) * STDEV(AI_Integration_Level) * STDEV(Citizen_Satisfaction_Pct))
    ) AS Pearson_R_AI_Citizen
FROM
(
    SELECT AI_Integration_Level, Citizen_Satisfaction_Pct,
           AVG(AI_Integration_Level) OVER() AS AVG_AI,
           AVG(Citizen_Satisfaction_Pct) OVER() AS AVG_CS
    FROM dbo.v_CityReadiness
) t;
</CommandText>
      </Query>
      <Fields>
        <Field Name="Pearson_R_AI_Citizen">
          <rd:TypeName>System.Double</rd:TypeName>
          <DataField>Pearson_R_AI_Citizen</DataField>
        </Field>
      </Fields>
    </DataSet>
